{
  "name":    "simp-localusers",
  "version": "0.1.0",
  "author":  "SIMP Team",
  "summary": "A Puppet module to more effeciently create local users",
  "license": "Apache-2.0",
  "source":  "https://github.com/simp/pupmod-simp-localusers.git",
  "project_page": "https://github.com/simp/pupmod-simp-localusers",
  "issues_url":   "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "user",
    "users"
  ],
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.9.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "4.x"
    },
    {
      "name": "pe",
      "version_requirement": ">= 2016.2.0"
    }
  ]
